{
	"name": "@flyingcatband/tunebook",
	"version": "0.15.0-beta.6",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/flyingcatband/tunebook"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./types": {
			"types": "./dist/types/index.d.ts"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"default": "./dist/server/index.js"
		}
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-package",
		"preview": "vite build && vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && tsc --noEmit && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test --fully-parallel",
		"test:unit": "vitest --run",
		"prepare": "husky"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.36.0",
		"@playwright/test": "^1.51.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.46.4",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@types/node": "^20",
		"@typescript-eslint/eslint-plugin": "^8.46.0",
		"@typescript-eslint/parser": "^8.46.0",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"fast-check": "^4.2.0",
		"globals": "^16.4.0",
		"husky": "^9.1.7",
		"lint-staged": "16.2.4",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"sv": "0.9.7",
		"svelte-check": "^4.3.3",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.44.1",
		"vite": "7.1.9",
		"vitest": "^3.2.4"
	},
	"type": "module",
	"dependencies": {
		"abcjs": "^6.4.4",
		"drab": "7.0.3",
		"esm-env": "^1.2.2",
		"slugify": "^1.6.6",
		"svelte": "5.39.11"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx,svelte}": [
			"prettier --plugin-search-dir . --write ."
		]
	},
	"packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
