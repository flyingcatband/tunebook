{
	"name": "@flyingcatband/tunebook",
	"version": "0.5.0",
	"license": "MIT",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./types": {
			"types": "./dist/types/index.d.ts"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"default": "./dist/server/index.js"
		}
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-package",
